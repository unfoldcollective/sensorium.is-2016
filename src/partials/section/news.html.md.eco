<div class="section-part-news mt">
  <h2 class="section-title">News</h2>
  <% for post in @getCollection("posts").findAll({isLive:true}).sortArray(date:-1): %>
  <ul>
    <li>
    <p class="label label-news"><%= post.humanDate %></p>
    <a href="<%=post.url %>"><%= post.title %></a>
    </li>
  </ul>
  <% end %>
</div>